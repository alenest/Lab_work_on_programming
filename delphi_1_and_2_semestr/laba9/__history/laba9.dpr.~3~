program laba9;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

const

  nmax = 10;

  var
  i, j, errortype, n : integer;
  test, answer : TextFile;
   a : array[1..nmax,1..nmax] of currency;


begin
  readln;
  try // пробую открыть файл на чтение

  //readln;
      AssignFile(test, 'POHUI_MNE.txt');
     //AssignFile(test, ParamSTR(1));
     reset(test);
     try
        read(n);
       for i := 1 to n do
      begin
          for j := 1 to n do read(test,a[i,j]);
          readln(test);

      end;
     except

     end;
     //reset(test);

      //writeln('pupupupu');
  except
      //writeln('pampampam');
      errortype := 1;
  end;
  //writeln('blyaaaaaaaaaaaa');

  case errortype of
  1: writeln('Не удалось открыть файл для считывания данных');
  2: writeln('Ошибка ввода из файла');
  end;

  readln;

end.
